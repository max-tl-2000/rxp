{
    "main": "node_modules/expo/AppEntry.js",
    "bnrConfig": "./tasks.js",
    "scripts": {
        "start": "./customize.sh && expo start",
        "android": "npm run start --android",
        "ios": "npm run start --ios",
        "web": "npm run start --web",
        "eject": "expo eject",
        "lint": "./customize.sh && eslint --cache --cache-location=node_modules/.cache/ --ext .js,.ts,.tsx .",
        "lint:fix": "npm run lint -- --fix",
        "autofix": "npm run lint:fix",
        "test": "jest",
        "e2e-setup-web": "expo start:web",
        "e2e-smoke-tests-web": "jest --silent ./__tests__/e2e/suites/smoke/*.* ",
        "verify": "tsc --project tsconfig.json --noEmit && yarn lint",
        "expo": "expo",
        "changelog": "changelogx -f markdown -o ./changelog.md -c ./changelogx.config.js",
        "do-changelog": "npm run changelog && git add ./changelog.md && git commit -m 'DOC: Generate changelog' --no-verify || true",
        "predeploy": "./customize.sh && expo build:web",
        "deploy-staging-firebase": "npm run predeploy && firebase deploy --token {firebase-token}",
        "postinstall": "patch-package || true"
    },
